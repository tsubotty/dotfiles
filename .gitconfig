[user]
	name = Seiya Tsubone
	email = karasuhaneto@gmail.com
[core]
	excludesfile = /Users/tsubone/.gitignore_global
	autocrlf = input
	editor = vim
[color]
	status = auto
	diff = auto
	branch = auto
	interactive = auto
	grep = auto
[alias]
	co = checkout
	st = status
	ci = commit -a
	ft = fetch
	vi = "!f() { vi "$(git ls-files "\\*$1\\*" | head -10)"; }; f" 
	sh = show
	br = branch
	dif = diff
	re= remote
[tig "bind"]
	generic = g move-first-line
	generic = G move-last-line
[github]
	user = karasuhaneto@gmail.com
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = upstream
