[core]
	excludesfile = /Users/tsubone/.gitignore_global
	autocrlf = input
	editor = vim
[include]
    path = ~/Dropbox/Misc/.gitconfig.local
[color]
	status = auto
	diff = auto
	branch = auto
	interactive = auto
	grep = auto
[alias]
	co = checkout
	st = status
	ci = commit -a
	ft = fetch
	vi = "!f() { vi "$(git ls-files "\\*$1\\*" | head -10)"; }; f" 
	sh = show
	br = branch
	dif = diff
	re = remote
	switch = checkout -
	sw = checkout -
[tig "bind"]
	generic = g move-first-line
	generic = G move-last-line
[push]
	default = upstream
