[user]
	name = Tsubone Seiya
	email = karasuhaneto@gmail.com
[core]
	excludesfile = $HOME/.gitignore
	autocrlf = input
	editor = vim
[color]
	status = auto
	diff = auto
	branch = auto
	interactive = auto
	grep = auto
[alias]
	co = checkout
	st = status
	ci = commit -a
	ft = fetch
	vi = "!f() { vi "$(git ls-files "\\*$1\\*" | head -10)"; }; f" 
[tig "bind"]
	generic = g move-first-line
	generic = G move-last-line
